{
  "name": "repeated-calls",
  "version": "1.0.0",
  "description": "Repeated calls to the function",
  "keywords": [
    "promise"
  ],
  "bugs": {
    "url": "https://github.com/Krivega/repeated-calls/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Krivega/repeated-calls.git"
  },
  "license": "MIT",
  "author": {
    "name": "Krivega Dmitriy",
    "email": "mr.krivega@gmail.com",
    "url": "https://krivega.com"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "files": [
    "dist/**/*",
    "README.md"
  ],
  "scripts": {
    "build": "NODE_ENV=production rollup --config",
    "format": "prettier --write \"src/**/*.js\"",
    "lint": "eslint src/. --ext .js",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "release": "standard-version",
    "test": "jest"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@rollup/plugin-node-resolve": "^11.1.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.18.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "husky": "^4.3.8",
    "jest": "26.6.3",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "rollup": "^2.36.2",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-terser": "^7.0.2",
    "standard-version": "^9.1.0"
  },
  "main:src": "src/index.js",
  "standard-version": {
    "scripts": {
      "prerelease": "npm run lint && CI=true npm test",
      "postchangelog": "prettier --write \"CHANGELOG.md\""
    }
  }
}
